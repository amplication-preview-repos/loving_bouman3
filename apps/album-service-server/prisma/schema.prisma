datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Album {
  createdAt   DateTime @default(now())
  description String?
  event       String?
  id          String   @id @default(cuid())
  name        String?
  updatedAt   DateTime @updatedAt
}
