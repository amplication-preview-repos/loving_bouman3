datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Picture {
  album     String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String?
  s3Url     String?
  updatedAt DateTime @updatedAt
}
